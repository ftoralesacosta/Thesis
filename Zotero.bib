@article{Xie2021,
  title = {\$\textbackslash ensuremath\{\textbackslash gamma\}\$-Hadron Spectra in \$p\textbackslash phantom\{\textbackslash rule\{4pt\}\{0ex\}\}+\textbackslash phantom\{\textbackslash rule\{4pt\}\{0ex\}\}\textbackslash mathrm\{\vphantom\}{{Pb}}\vphantom\{\}\$ Collisions at \$\textbackslash sqrt\{\{s\}\_\{\vphantom{\}\}}{{NN}}\vphantom\{\}\vphantom\{\}=5.02\$ {{TeV}}},
  author = {Xie, Man and Wang, Xin-Nian and Zhang, Han-Zhong},
  date = {2021-03-29},
  journaltitle = {Phys. Rev. C},
  volume = {103},
  number = {3},
  pages = {034911},
  publisher = {{American Physical Society}},
  doi = {10.1103/PhysRevC.103.034911},
  url = {https://link.aps.org/doi/10.1103/PhysRevC.103.034911},
  urldate = {2021-11-05},
  abstract = {Under the assumption that a quark-gluon plasma (QGP) droplet is produced and its evolution can be described by hydrodynamics in p+A collisions, {$\gamma$}-triggered hadron spectra are studied within a next-to-leading-order perturbative QCD parton model with the medium-modified parton fragmentation functions. The initial conditions and space-time evolution of the small QGP droplet are provided by the superSONIC hydrodynamic model simulations and parton energy loss in such a medium is described by the high-twist (HT) approach. The range of scaled jet transport coefficient \textasciicircum q0/T30 in this HT approach is extracted from single hadron suppression in A+A collisions with similar initial medium temperature as in p+A collisions. Numerical results for this scenario show that {$\gamma$}-hadron spectra at p{$\gamma$}T=12\textendash 40GeV/c are suppressed by 5\textendash 15\% in the most central 0\textendash 10\% p+Pb collisions at {$\surd$}sNN=5.02 TeV. The suppression becomes weaker at higher transverse momentum of the {$\gamma$} trigger. As a comparison, {$\gamma$}-hadron suppression in Pb+Pb collisions at {$\surd$}sNN=2.76 and 5.02 TeV is also predicted.},
  file = {/Users/fernando/Zotero/storage/YTTY3F47/Xie et al. - 2021 - $ensuremath gamma $-hadron spectra in $pphantom.pdf;/Users/fernando/Zotero/storage/4JWUV33L/PhysRevC.103.html}
}

