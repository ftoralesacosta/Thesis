@article{epps16:2017,
  title = {{{EPPS16}}: Nuclear Parton Distributions with {{LHC}} Data},
  shorttitle = {{{EPPS16}}},
  author = {Eskola, Kari J. and Paakkinen, Petja and Paukkunen, Hannu and Salgado, Carlos A.},
  date = {2017-03},
  journaltitle = {The European Physical Journal C},
  volume = {77},
  number = {3},
  pages = {163},
  issn = {1434-6052},
  doi = {10.1140/epjc/s10052-017-4725-9},
  url = {https://doi.org/10.1140/epjc/s10052-017-4725-9},
  urldate = {2021-12-02},
  abstract = {We introduce a global analysis of collinearly factorized nuclear parton distribution functions (PDFs) including, for the first time, data constraints from LHC proton\textendash lead collisions. In comparison to our previous analysis, EPS09, where data only from charged-lepton\textendash nucleus deep inelastic scattering (DIS), Drell\textendash Yan (DY) dilepton production in proton\textendash nucleus collisions and inclusive pion production in deuteron\textendash nucleus collisions were the input, we now increase the variety of data constraints to cover also neutrino\textendash nucleus DIS and low-mass DY production in pion\textendash nucleus collisions. The new LHC data significantly extend the kinematic reach of the data constraints. We now allow much more freedom for the flavor dependence of uclear effects than in other currently available analyses. As a result, especially the uncertainty estimates are more objective flavor by flavor. The neutrino DIS plays a pivotal role in obtaining a mutually consistent behavior for both up and down valence quarks, and the LHC dijet data clearly constrain gluons at large momentum fraction. Mainly for insufficient statistics, the pion\textendash nucleus DY and heavy-gauge-boson production in proton\textendash lead collisions impose less visible constraints. The outcome \textendash{} a new set of next-to-leading order nuclear PDFs called EPPS16 \textendash{} is made available for applications in high-energy nuclear collisions.},
  langid = {english}
}

