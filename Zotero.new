  author = {Aidala, C. and Akiba, Y. and Alfred, M. and Andrieux, V. and Aoki, K. and Apadula, N. and Asano, H. and Ayuso, C. and Azmoun, B. and Babintsev, V. and Bagoly, A. and Bandara, N. S. and Barish, K. N. and Bathe, S. and Bazilevsky, A. and Beaumier, M. and Belmont, R. and Berdnikov, A. and Berdnikov, Y. and Blau, D. S. and Boer, M. and Bok, J. S. and Brooks, M. L. and Bryslawskyj, J. and Bumazhnov, V. and Butler, C. and Campbell, S. and Roman, V. Canoa and Cervantes, R. and Chi, C. Y. },
  options = {useprefix=true},
  date = {2019-03},
  journaltitle = {Nat. Phys.},
  volume = {15},
  number = {3},
  pages = {214--220},
  publisher = {{Nature Publishing Group}},
  issn = {1745-2481},
  doi = {10.1038/s41567-018-0360-0},
  url = {https://www.nature.com/articles/s41567-018-0360-0},
  urldate = {2021-10-14},
  abstract = {Experimental studies of the collisions of heavy nuclei at relativistic energies have established the properties of the quark\textendash gluon plasma (QGP), a state of hot, dense nuclear matter in which quarks and gluons are not bound into hadrons1\textendash 4. In this state, matter behaves as a nearly inviscid fluid5 that efficiently translates initial spatial anisotropies into correlated momentum anisotropies among the particles produced, creating a common velocity field pattern known as collective flow. In recent years, comparable momentum anisotropies have been measured in small-system proton\textendash proton (p+p) and proton\textendash nucleus (p+A) collisions, despite expectations that the volume and lifetime of the medium produced would be too small to form a QGP. Here we report on the observation of elliptic and triangular flow patterns of charged particles produced in proton\textendash gold (p+Au), deuteron\textendash gold (d+Au) and helium\textendash gold (3He+Au) collisions at a nucleon\textendash nucleon centre-of-mass energy {$\$\$\backslash$}sqrt {$\lbrace$}s{$\_\lbrace\lbrace\backslash$}mathrm{$\lbrace$}NN{$\rbrace\rbrace\rbrace\rbrace\$\$$}\,=\,200\,GeV. The unique combination of three distinct initial geometries and two flow patterns provides unprecedented model discrimination. Hydrodynamical models, which include the formation of a short-lived QGP droplet, provide the best simultaneous description of these measurements.},
  langid = {english},
  file = {/Users/fernando/Zotero/storage/AS6KRAMR/Aidala et al. - 2019 - Creation of quark–gluon plasma droplets with three.pdf;/Users/fernando/Zotero/storage/6XUKSV6E/s41567-018-0360-0.html}
}

