@article{Aaij2016,
  title = {Measurements of {{Long}}-{{Range}} near-{{Side Angular Correlations}} in {{sNN}}=5 {{TeV}} {{Proton}}-{{Lead Collisions}} in the {{Forward Region}}},
  author = {Aaij, R. and Abell\'an Beteta, C. and Adeva, B. and Adinolfi, M. and Affolder, A. and Ajaltouni, Z. and Akar, S. and Albrecht, J. and Alessio, F. and Alexander, M. and Ali, S. and Alkhazov, G. and Alvarez Cartelle, P. and Alves, A. A. and Amato, S. and Amerio, S. and Amhis, Y. and An, L. and Anderlini, L. },
  options = {useprefix=true},
  date = {2016-11-10},
  journaltitle = {Physics Letters B},
  volume = {762},
  pages = {473--483},
  issn = {0370-2693},
  doi = {10.1016/j.physletb.2016.09.064},
  url = {https://www.sciencedirect.com/science/article/pii/S037026931630572X},
  urldate = {2021-10-15},
  abstract = {Two-particle angular correlations are studied in proton-lead collisions at a nucleon\textendash nucleon centre-of-mass energy of sNN=5 TeV, collected with the LHCb detector at the LHC. The analysis is based on data recorded in two beam configurations, in which either the direction of the proton or that of the lead ion is analysed. The correlations are measured in the laboratory system as a function of relative pseudorapidity, {$\Delta\eta$}, and relative azimuthal angle, {$\Delta\phi$}, for events in different classes of event activity and for different bins of particle transverse momentum. In high-activity events a long-range correlation on the near side, {$\Delta\phi\approx$}0, is observed in the pseudorapidity range 2.0{$<\eta<$}4.9. This measurement of long-range correlations on the near side in proton-lead collisions extends previous observations into the forward region up to {$\eta$}=4.9. The correlation increases with growing event activity and is found to be more pronounced in the direction of the lead beam. However, the correlation in the direction of the lead and proton beams are found to be compatible when comparing events with similar absolute activity in the direction analysed.},
  langid = {english},
  file = {/Users/fernando/Zotero/storage/P26F7BN5/Aaij et al. - 2016 - Measurements of long-range near-side angular corre.pdf;/Users/fernando/Zotero/storage/72IFS9LR/S037026931630572X.html}
}

