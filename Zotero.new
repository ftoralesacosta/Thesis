@article{Liang2008,
  title = {The {{Transverse}}-{{Momentum}}-{{Dependent}} {{Parton Distribution Function}} and {{Jet Transport}} in {{Medium}}},
  author = {Liang, Zuo-tang and Wang, Xin-Nian and Zhou, Jian},
  date = {2008-06-06},
  journaltitle = {Phys. Rev. D},
  volume = {77},
  number = {12},
  eprint = {0801.0434},
